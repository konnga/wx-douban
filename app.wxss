/**app.wxss**/

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}

/*icon-front  */
@font-face {
    font-family: 'iconfont';
    src: url('iconfont.eot');
    font-weight: normal;
    font-style: normal;
}
@font-face {
    font-family: 'iconfont';
    src: url(data:application/font-woff;charset=utf-8;base64,) format('woff'),
        url('iconfont.ttf') format('truetype');
    font-weight: normal;
    font-style: normal;
}
.iconfont {
  font-family:"iconfont" !important;
  font-size:16px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-arrow-right:before { content: "\e604"; }

.icon-star:before { content: "\e62c"; }

.icon-moive:before { content: "\e602"; }

.icon-my:before { content: "\e6ed"; }

.icon-sousuo:before { content: "\e62e"; }

.icon-my-active:before { content: "\e6ee"; }

.icon-moive-active:before { content: "\e6ef"; }

.icon-star-active:before { content: "\e6f0"; }