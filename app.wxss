.container {
  width: 100%;
  height: 100%;
}
/*公共样式  */
.text-overflow {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.light-color {
  color: #fadb14;
}
.init-color {
  color: #27a6e5;
}
.gray-color {
  color: #DCDCDC;
}
.center {
  text-align: center;
  margin: 0 auto;
}
.weight {
  font-weight: 600;
}
.font-large {
  font-size: 48rpx;
}
.font-medium {
  font-size: 32rpx;
}
.font-small {
  font-size: 28rpx;
}
.right {
  float: right;
}
/*end  */

/*transfonter  */
@font-face {
    font-family: 'iconfont';
    src: url('iconfont.eot');
    font-weight: normal;
    font-style: normal;
}
@font-face {
    font-family: 'iconfont';
    src: url(data:application/font-woff;charset=utf-8;base64,) format('woff'),
        url('iconfont.ttf') format('truetype');
    font-weight: normal;
    font-style: normal;
}
/*iconfont  */
.iconfont {
  font-family:"iconfont" !important;
  font-size:16px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-arrow-right:before { content: "\e604"; }

.icon-star-half:before { content: "\e6f1"; }

.icon-star:before { content: "\e62c"; }

.icon-moive:before { content: "\e602"; }

.icon-my:before { content: "\e6ed"; }

.icon-sousuo:before { content: "\e62e"; }

.icon-my-active:before { content: "\e6ee"; }

.icon-moive-active:before { content: "\e6ef"; }

.icon-star-active:before { content: "\e6f0"; }